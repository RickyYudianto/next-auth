#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@fauna-labs+fauna-schema-migrate@2.2.1_faunadb@4.6.0/node_modules/@fauna-labs/fauna-schema-migrate/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@fauna-labs+fauna-schema-migrate@2.2.1_faunadb@4.6.0/node_modules/@fauna-labs/fauna-schema-migrate/dist/cli.js" "$@"
fi
