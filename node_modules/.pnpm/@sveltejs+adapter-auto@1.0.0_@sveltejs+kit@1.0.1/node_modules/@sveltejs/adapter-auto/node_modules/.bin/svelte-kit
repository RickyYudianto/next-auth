#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sveltejs+kit@1.0.1_svelte@3.54.0+vite@4.0.1/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../../../../../../@sveltejs+kit@1.0.1_svelte@3.54.0+vite@4.0.1/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
fi
