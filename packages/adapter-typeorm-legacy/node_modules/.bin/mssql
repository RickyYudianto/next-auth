#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/mssql@7.3.5/node_modules/mssql/bin/mssql" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/mssql@7.3.5/node_modules/mssql/bin/mssql" "$@"
fi
