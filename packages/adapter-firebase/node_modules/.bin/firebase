#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ricky.yudianto/Documents/Private/next-auth/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/firebase-tools@11.16.1/node_modules/firebase-tools/lib/bin/firebase.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/firebase-tools@11.16.1/node_modules/firebase-tools/lib/bin/firebase.js" "$@"
fi
