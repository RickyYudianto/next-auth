#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../install-artifact-from-github@1.3.1/node_modules/install-artifact-from-github/bin/install-from-cache.js" "$@"
else
  exec node  "$basedir/../../../../../install-artifact-from-github@1.3.1/node_modules/install-artifact-from-github/bin/install-from-cache.js" "$@"
fi
